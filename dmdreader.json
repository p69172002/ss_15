{
  "general": {
    "cwd_to_configdir": true,
    "log": ""
  },
  "source": {
    "spi": {
      "device": "/dev/spidev0.0",
      "speed": 4000000,
      "notify_gpio": 17
    }
  },
  "processor": {
    "serum": {
      "file": "/etc/ss_15.crz",
      "bits_per_pixel": 4,
      "maximum_unknown_frames_to_skip": 5
    } 
  },
  "renderer": {
    "ledmatrix": {
      "width": 128,
      "height": 32,
      "rgb_sequence": "rbg",
      "brightness": 70,
      "pwm_bits": 9
    }
  }
}
